{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run wheresmyprompt",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": []
        },
        {
            "name": "Get wheresmyprompt version",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["version"]
        },
        {
            "name": "All Documentation",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["-a", "documentation"]
        },
        {
            "name": "Golang,Starter Section One-Shot",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["-s", "Golang,starter", "-o"]
        }
    ]
}